<launch>
  <node name="mpu9250" pkg="mpu9250" type="mpu9250" output="screen" />
  <node name="deadreckoning" pkg="deadreckoning" type="deadreckoning" />

  <node name="pid_control" pkg="nemcon" type="pid_control" output="screen">
    <param name="speed" value="0" />
    <param name="front" value="1" />
    <param name="imu_P" value="20.0" />
    <param name="imu_I" value="2.00" />
    <param name="imu_D" value="0.05" />
    <param name="enc_P" value="0.00" />
    <param name="enc_I" value="0.00" />
    <param name="enc_D" value="0.00" />
  </node>
</launch>
